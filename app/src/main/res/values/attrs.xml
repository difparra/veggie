<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--    Attributes for QuantityButton   -->
    <declare-styleable name="QtyButton">
        <attr name="enabled_behaviour" format="enum">
            <enum name="add" value="0"/>
            <enum name="reduce" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="qty_states">
        <attr name="state_qty_zero" format="boolean" />
        <attr name="state_qty_one" format="boolean" />
        <attr name="state_qty_normal" format="boolean" />
        <attr name="state_qty_max" format="boolean" />
    </declare-styleable>

</resources>